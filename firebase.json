{
  "functions": [
    {
      "source": "shared-functions",
      "codebase": "default",
      "ignore": [
        "node_modules",
        ".git",
        "firebase-debug.log",
        "firebase-debug.*.log"
      ]
    }
  ],
  "hosting": {
    "target": "southport-carpet-centre",
    "public": "southport-carpet-centre/public",
    "rewrites": [
      {
        "source": "/api/sendMailOverHTTP",
        "function": "sendMailOverHTTP"
      }
    ],
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ]
  }
}
